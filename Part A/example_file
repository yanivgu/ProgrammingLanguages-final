#calculate fibonacci series on n items
fib_1 = 1
fib_2 = 1
n = 20
while n > 0
    print(fib_1)
    temp = fib_1
    fib_1 = fib_2
    fib_2 = fib_2 + temp
    n = n - 1
endwhile